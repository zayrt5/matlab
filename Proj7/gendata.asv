function pairs = gendata(npoints,range, theta)
%GENDATA Summary of this function goes here
%   Detailed explanation goes here
    k= 1;
    pairs = (2)
    for x = range(1):(range(2)-range(1))/(npoints-1):range(2)
        y = thetaexp(theta,x); 
        disp(k);
        disp(x);
        disp(y);
        pairs(k,1) = x;
        pairs(k,2) = y;
        k = k + 1;
    end
end

